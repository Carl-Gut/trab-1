Algoritmo medidas
	Escribir "ingrese el valor a convertir: "
	Leer v;
	yard<-(v/91.44);
	metr<-v/100;
	pie<-v/30.48;
	pul<-v/2.54;
	Escribir "El valor dado en yardas es: ",redon(yard);
	Escribir "El valor dado en metros es: ",redon(metr);
	Escribir "El valor dado en pie es: ",redon(pie);
	Escribir "El valor dado en pulgadas es de: ",redon(pul);
FinAlgoritmo
